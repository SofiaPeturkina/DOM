{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
        ],
        "eol-last": [
            "error", 
            "never"
        ],
        "no-multiple-empty-lines": [
            "error", { "max": 1, "maxEOF": 0 }
        ]
    }
}
